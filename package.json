{
  "name": "formation-angular",
  "description": "Formation Angular ",
  "version": "0.0.1",
  "author": "Zenika",
  "homepage": "https://www.zenika.com/Formation/notre-catalogue-formation.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zenika-Training/formation-angular"
  },
  "private": true,
  "dependencies": {
    "grunt": "^1.3.0",
    "gulp": "^4.0.2",
    "zenika-formation-framework": "^3.5.0"
  },
  "config": {
    "deploy": {
      "name": "zen-formation-angular"
    }
  },
  "scripts": {
    "start": "grunt",
    "pdf": "grunt pdf",
    "deploy": "zenika-formation-deploy",
    "sensei-start": "docker run -it --rm -p 8080:8080 -v $(pwd):/training-material zenika/sensei",
    "sensei-pdf": "docker run -it --rm -p 8080:8080 -v $(pwd):/training-material --cap-add SYS_ADMIN zenika/sensei pdf"
  },
  "devDependencies": {
    "@angular/cli": "^12.2.6",
    "grunt-cli": "^1.3.2"
  }
}
